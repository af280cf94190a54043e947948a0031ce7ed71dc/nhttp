{
  "name": "nhttp-land",
  "description": "An Simple http framework for Deno and Friends",
  "author": "Herudi",
  "version": "1.1.18",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./types/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhttp/nhttp.git"
  },
  "bugs": {
    "url": "https://github.com/nhttp/nhttp/issues"
  },
  "engines": {
    "node": ">=18.14.0"
  },
  "keywords": [
    "nhttp",
    "nhttp-land",
    "deno",
    "nodejs",
    "bun",
    "cloudflare-workers",
    "router",
    "middleware",
    "framework"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./serve-static": {
      "types": "./types/lib/serve-static.d.ts",
      "require": "./dist/cjs/lib/serve-static.js",
      "import": "./dist/esm/lib/serve-static.js"
    },
    "./swagger": {
      "types": "./types/lib/swagger.d.ts",
      "require": "./dist/cjs/lib/swagger.js",
      "import": "./dist/esm/lib/swagger.js"
    },
    "./etag": {
      "types": "./types/lib/etag.d.ts",
      "require": "./dist/cjs/lib/etag.js",
      "import": "./dist/esm/lib/etag.js"
    },
    "./deps": {
      "types": "./types/lib/deps.d.ts",
      "require": "./dist/cjs/lib/deps.js",
      "import": "./dist/esm/lib/deps.js"
    },
    "./controller": {
      "types": "./types/lib/controller.d.ts",
      "require": "./dist/cjs/lib/controller.js",
      "import": "./dist/esm/lib/controller.js"
    },
    "./class-validator": {
      "types": "./types/lib/class-validator.d.ts",
      "require": "./dist/cjs/lib/class-validator.js",
      "import": "./dist/esm/lib/class-validator.js"
    }
  },
  "typesVersions": {
    "*": {
      "./serve-static": [
        "./types/lib/serve-static.d.ts"
      ],
      "./swagger": [
        "./types/lib/swagger.d.ts"
      ],
      "./etag": [
        "./types/lib/etag.d.ts"
      ],
      "./deps": [
        "./types/lib/deps.d.ts"
      ],
      "./controller": [
        "./types/lib/controller.d.ts"
      ],
      "./class-validator": [
        "./types/lib/class-validator.d.ts"
      ]
    }
  }
}